Host *
     ServerAliveInterval 60
     ServerAliveCountMax 30

## override as per host ##

Host pizero
     HostName 192.168.178.35
     User pi
     IdentityFile ~/.ssh/pizero

Host pi
    HostName 192.168.178.100
    User pi
    Port 22
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/pibuntu

Host kindle
     HostName 192.168.178.26
     User root
     IdentityFile ~/.ssh/kindle

Host tplink
    HostName 192.168.178.2
    user root
    IdentityFile ~/.ssh/tplink

