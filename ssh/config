    user root
    IdentityFile ~/.ssh/tplink

Host github.com-DonPavlov
    HostName github.com
	PreferredAuthentications publickey
    User git
    IdentityFile ~/.ssh/github_ecdsa
    
Host gitlab.com
   HostName gitlab.com
   PreferredAuthentications publickey
   IdentityFile ~/.ssh/github_eos
